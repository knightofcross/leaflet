{
    "name": "nailfor/leaflet",
    "description": "LeafLet Mobile-Friendly Interactive Maps Extension Library for Laravel",
    "type": "library",
    "keywords": [
        "nailfor",
        "laravel",
        "leaflet",
        "extension",
        "maps"
    ],
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/nailfor/leaflet",
    "authors": [
        {
            "name": "nailfor",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/nailfor/leaflet/issues",
        "source": "https://github.com/nailfor/leaflet"
    },
    "require": {
        "illuminate/view": "~5.5",
        "bower-asset/leaflet": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "nailfor\\leaflet\\": "src/Models"
        }
    },
    "extra": {
	"laravel": {
	    "providers": [
		"nailfor\\leaflet\\Providers\\MapServiceProvider"
	    ]
	},
	"branch-alias": {
	    "dev-master": "1.0.x-dev"
	}
    },
    "prefer-stable": true
}
